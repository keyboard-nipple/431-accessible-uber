<h1>Admins#index</h1>
<table>
    <tr>
    <th>Name</th> <br>
    
  </tr>
  
<% @admins.each do |admin| %>
    <tr>
      <td><%= admin.name %></td>
      <td><%= link_to 'Show', admin_path(admin) %></td>
      <td><%= link_to 'Edit', edit_admin_path(admin) %></td>
      <td><%= link_to 'Destroy', admin_path(admin),
        method: :delete,
        data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>

